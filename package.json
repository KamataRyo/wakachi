{
  "name": "wakachi",
  "version": "0.0.0",
  "description": "日本語文の改行位置を調整するjQueryプラグイン",
  "main": "Wakachi.js",
  "dependencies": {
    "jquery": "1.12.4"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "coffee-script": "^1.11.1",
    "gulp": "^3.9.1",
    "mocha": "^2.5.3",
    "should": "^9.0.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintignore": [
    "TynySegmenter.js"
  ],
  "scripts": {
    "build": "gulp build",
    "lint": "coffeelint gulpfile.coffee Wakachi.coffee tests/test.coffee",
    "install": "npm run build",
    "test": "mocha --compilers coffee:coffee-script/register -- test/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "KamataRyo",
  "license": "MIT",
  "homepage": "http://biwako.io/wakachi"
}
